题3： 
题4：
#include<stdio.h>
struct English{
	char letter;
	struct English *next;
	struct English *last;
	 
};
int main()
{
	struct English head,*p,*q,*tail,*j;
	int i;
	j=&head;
	j->next=p;
	p->last=j;
	j->last=NULL;
	for(i=0;i<26;i++)
	{
	p=(struct English *)malloc(sizeof(struct English));
	scanf("%c",&p->letter);
	p->next=q;
	q->last=p;
	p=p->next;
	p->next=tail;
	} 
	p=tail->last;
	
	
		printf("%c",p->letter);
	

	return 0;
}

